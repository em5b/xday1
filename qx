function inv()
    Bd()
    equ()
    -- [ Locals ] --
    local Player = game.Players.LocalPlayer
    local Char = Player.Character
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name == "Union" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Beam" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "B1" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "B2" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "BB1" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "BB2" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "ParticleEmitter" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Trail" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Trail" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Trail0" then
            v:Destroy()
     end
    end
    for i,v in pairs(Char:GetDescendants()) do
        if v.Name ==  "Trail1" then
            v:Destroy()
     end
    end
    end

function grabtools()
    local Human = plr.Character:FindFirstChildWhichIsA("Humanoid")
    for _, v in ipairs(workspace:GetChildren()) do
        if plr.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
            Human:EquipTool(v)
        end
    end
    if grabtoolsFunc then grabtoolsFunc:Disconnect() end
    grabtoolsFunc = workspace.ChildAdded:Connect(function(v)
        if plr.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
            plr.Character.Humanoid:EquipTools()
        end
    end)
end
function fre()
        Bd()
        plr.Character.Humanoid.Health = 0
        wait(.1)
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer("/re")
end

function ungrabtools()
    if grabtoolsFunc then grabtoolsFunc:Disconnect() end
end


